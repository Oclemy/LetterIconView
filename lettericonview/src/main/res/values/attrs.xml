<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LetterIconView">
        <attr name="liv_backgroundColor" format="color" />
        <attr name="liv_letters" format="string" />
        <attr name="liv_isGradient" format="boolean" />
        <attr name="liv_shape" format="enum">
            <enum name="square" value="0"/>
            <enum name="round_square" value="1"/>
            <enum name="circle" value="2"/>
        </attr>
    </declare-styleable>

</resources>